-none text-wrap d-inline-block\"\r\n          target=\"_blank\"\r\n          :href=\"item.link\"\r\n          >G<i class=\"fa fa-link\" aria-hidden=\"true\"></i\r\n        ></a>\r\n        <button\r\n          @click=\"\r\n            updateCurrentMapItem(item)\r\n            checkIfNeedShowPopup()\r\n          \"\r\n          v-if=\"item.gpid\"\r\n          type=\"button\"\r\n          class=\"btn btn-secondary btn-sm ml-3\"\r\n        >\r\n          <i class=\"fa fa-ban\" aria-hidden=\"true\"></i>\r\n        </button>\r\n      </template>\r\n      <template v-slot:cell(discovered_gpids)=\"{ item, value, index }\">\r\n        <ul\r\n          class=\"mb-0 w-100 p-0\"\r\n          v-for=\"(itemGpid, indexGpid) in item.discovered_gpids\"\r\n          :key=\"indexGpid\"\r\n          style=\"list-style-position: inside; list-style-type: none\"\r\n        >\r\n          <li class=\"max-width-300\">\r\n            <div class=\"d-inline\" v-if=\"item.discovered_gpids.length > 1\">\r\n              {{ indexGpid + 1 }}.\r\n            </div>\r\n            <div\r\n              v-if=\"\r\n                !itemGpid.status ||\r\n                itemGpid.status === GoogleShoppingMapStatus.NORMAL\r\n              \"\r\n              class=\"badge badge-info\"\r\n            >\r\n              {{ itemGpid.gpid }}\r\n            </div>\r\n            <div\r\n              v-if=\"itemGpid.status === GoogleShoppingMapStatus.DENIED\"\r\n              class=\"badge badge-info\"\r\n            >\r\n              <del>{{ itemGpid.gpid }}</del>\r\n            </div>\r\n            on\r\n            <div class=\"badge badge-secondary\">\r\n              ({{ itemGpid.date | moment('MM/DD/YYYY') }})\r\n            </div>\r\n            <div class=\"badge badge-secondary text-wrap font-weight-normal\">\r\n              {{ itemGpid.title }}\r\n            </div>\r\n            <a\r\n              v-if=\"item.asin && item.asin != ''\"\r\n              class=\"ml-2 text-decoration-none text-wrap d-inline-block\"\r\n              target=\"_blank\"\r\n              :href=\"generateAmazonLink(item.asin)\"\r\n              >A<i class=\"fa fa-link\" aria-hidden=\"true\"></i\r\n            ></a>\r\n            <a\r\n              class=\"ml-2 text-decoration-none text-wrap d-inline-block\"\r\n              v-if=\"itemGpid.gpid && itemGpid.link && itemGpid.link !== ''\"\r\n              target=\"_blank\"\r\n              :href=\"itemGpid.link\"\r\n              >G<i class=\"fa fa-link\" aria-hidden=\"true\"></i\r\n            ></a>\r\n            <div class=\"btn-group ml-2\" role=\"group\" aria-label=\"Basic example\">\r\n              <button\r\n                type=\"button\"\r\n                :disabled=\"item.gpid == itemGpid.gpid\"\r\n                @click=\"\r\n                  updateCurrentMapItem(item, itemGpid)\r\n                  $bvModal.show('modal-confirm-clone-gpid')\r\n                \"\r\n                class=\"btn btn-success btn-sm\"\r\n              >\r\n                <i class=\"fa fa-check text-white\" aria-hidden=\"true\"></i>\r\n              </button>\r\n              <button\r\n                @click=\"\r\n                  updateCurrentMapItem(item, itemGpid)\r\n                  $bvModal.show('modal-confirm-remove-discovered-gpid')\r\n                \"\r\n                type=\"button\"\r\n                class=\"btn btn-secondary btn-sm\"\r\n              >\r\n                <i class=\"fa fa-ban\" aria-hidden=\"true\"></i>\r\n              </button>\r\n            </div>\r\n          </li>\r\n        </ul>\r\n      </template>\r\n      <template v-slot:cell(active)=\"row\">\r\n        <b-form-checkbox\r\n          v-model=\"row.item.active\"\r\n          :disabled=\"true\"\r\n        ></b-form-checkbox>\r\n      </template>\r\n      <template v-slot:cell(last_violation_checked)=\"data\">\r\n        <div class=\"float-left\" v-if=\"data.item.has_processed\">\r\n          {{ data.item.last_violation_checked | moment('MM/DD/YYY